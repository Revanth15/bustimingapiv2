{
    "version": 2,
    "builds": [
      { "src": "main.py", "use": "@vercel/python" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/main.py" }
    ],
    "env": {
      "APP_MODULE": "main:app"
    },
    "headers": [
    {
      "source": "/getBusRoutesData",
      "headers": [
        { "key": "Cache-Control", "value": "public, s-maxage=86400, stale-while-revalidate=86400" }
      ]
    }
  ]
  }
  